{
	"name": "amazon-location-features-demo-web",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"files": [
		"lib"
	],
	"main": "./lib/index.es.js",
	"module": "./lib/index.es.js",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.es.js",
			"require": "./lib/index.es.js"
		},
		"./style.css": {
			"import": "./lib/style.css",
			"require": "./lib/style.css"
		}
	},
	"scripts": {
		"dev": "vite",
		"lib": "tsc && vite build --config vite.library.ts",
		"local-lib": "tsc && vite build --config vite.library.ts && node extra/prepare-local-lib/index.js",
		"build": "tsc && vite build && cp -r ./public/.well-known/ ./build/",
		"cypress": "cypress run --browser chrome --headed",
		"cypress run": "cypress run",
		"cypress open": "cypress open",
		"prepare": "husky install",
		"test": "react-scripts test",
		"ci-test": "CI=true react-scripts test",
		"coverage": "react-scripts test --coverage --watchAll",
		"security-tests": "cd security-tests && npm install && node index.js && cd ../",
		"stylelint-check": "stylelint-config-prettier-check",
		"translate-content": "cd extra/translation-script-content && npm install && node index.js && cd ../../",
		"translate-content-update": "cd extra/translation-script-content && npm install && node update-translation.js && cd ../../"
	},
	"dependencies": {
		"@aws-amplify/ui-react": "3.6.1",
		"@aws-sdk/client-cognito-identity": "3.496.0",
		"@aws-sdk/client-iot": "3.496.0",
		"@aws-sdk/client-location": "3.496.0",
		"@aws-sdk/client-pinpoint": "3.496.0",
		"@aws-sdk/credential-providers": "3.370.0",
		"@turf/turf": "6.5.0",
		"aws-amplify": "4.3.43",
		"date-fns": "2.29.3",
		"i18next": "23.2.8",
		"i18next-browser-languagedetector": "7.1.0",
		"mapbox-gl-draw-circle": "1.1.2",
		"ngeohash": "0.6.3",
		"ramda": "0.28.0",
		"react": "18.2.0",
		"react-device-detect": "2.2.3",
		"react-dom": "18.2.0",
		"react-i18next": "13.0.1",
		"react-router-dom": "6.4.3",
		"react-spring-bottom-sheet": "3.5.0-alpha.0",
		"react-toastify": "9.1.1",
		"react-tooltip": "5.10.0",
		"zustand": "4.3.9"
	},
	"devDependencies": {
		"@faker-js/faker": "7.6.0",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/react": "13.4.0",
		"@testing-library/user-event": "13.5.0",
		"@types/mapbox__mapbox-gl-draw": "1.3.3",
		"@types/ngeohash": "0.6.4",
		"@types/node": "18.11.9",
		"@types/ramda": "0.28.18",
		"@types/react": "18.0.17",
		"@types/react-dom": "18.0.6",
		"@types/zen-observable": "0.8.3",
		"@typescript-eslint/eslint-plugin": "5.33.1",
		"@typescript-eslint/parser": "5.33.1",
		"@vitejs/plugin-react": "2.1.0",
		"cypress": "12.10.0",
		"cypress-browser-permissions": "1.1.0",
		"eslint": "8.22.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-flowtype": "8.0.3",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsx-a11y": "6.6.1",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-react": "7.30.1",
		"eslint-plugin-react-hooks": "4.6.0",
		"husky": "8.0.0",
		"prettier": "2.7.1",
		"react-scripts": "5.0.1",
		"sass": "1.57.1",
		"stylelint": "14.15.0",
		"stylelint-config-airbnb": "0.0.0",
		"stylelint-config-prettier": "9.0.4",
		"stylelint-config-standard": "29.0.0",
		"stylelint-order": "5.0.0",
		"stylelint-prettier": "2.0.0",
		"typescript": "4.9.3",
		"vite": "3.2.7",
		"vite-plugin-dts": "2.3.0",
		"vite-plugin-eslint": "1.7.0",
		"vite-plugin-inspect": "^0.8.1",
		"vite-plugin-svgr": "2.2.1"
	},
	"engines": {
		"node": ">=14.x.x"
	},
	"jest": {
		"moduleNameMapper": {
			"^@demo/(.*)$": "<rootDir>/src/$1",
			"\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.ts"
		},
		"collectCoverageFrom": [
			"**/*.{js,jsx,ts,tsx}",
			"!**/node_modules/**",
			"!**/src/**/index.ts",
			"!**/src/assets/**",
			"!**/src/atomicui/molecules/Marker/**",
			"!**/src/atomicui/molecules/SuggestionMarker/**",
			"!**/src/atomicui/pages/DemoPage/**",
			"!**/src/core/**",
			"!**/src/hooks/**",
			"!**/src/services/**",
			"!**/src/theme/**",
			"!**/src/types/**",
			"!**/src/utils/**",
			"!**/src/App.tsx",
			"!**/src/main.tsx",
			"!**/src/setupTests.ts",
			"!**/src/vite-env.d.ts",
			"!**/src/atomicui/organisms/AuthGeofenceBox/CircleDrawControl.tsx",
			"!**/src/atomicui/pages/DemoPlaceholderPage/**",
			"!**/src/atomicui/organisms/AuthTrackerBox/AuthTrackerSimulation.tsx",
			"!**/src/atomicui/organisms/UnauthSimulation/UnauthGeofencesSimulation.tsx",
			"!**/src/atomicui/organisms/UnauthSimulation/UnauthRouteSimulation.tsx",
			"!**/src/atomicui/organisms/Explore/**",
			"!**/src/atomicui/organisms/ResponsiveBottomSheet/**"
		]
	}
}

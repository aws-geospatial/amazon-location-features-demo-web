		"moduleNameMapper": {
			"^@demo/(.*)$": "<rootDir>/src/$1",
			"\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.ts"
		},
		"collectCoverageFrom": [
			"**/*.{js,jsx,ts,tsx}",
			"!**/node_modules/**",
			"!**/src/**/index.ts",
			"!**/src/assets/**",
